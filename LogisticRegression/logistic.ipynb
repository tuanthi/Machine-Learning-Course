   "source": [
    "N = 100\n",
    "D = 2\n",
    "\n",
    "X = np.random.randn(N,D)\n",
    "ones = np.array([[1]*N]).T\n",
    "Xb = np.concatenate((X, ones), axis=1)\n",
    "\n",
    "w = np.random.randn(D + 1)\n",
    "z = Xb.dot(w)\n",
    "\n",
    "def sigmoid(z):\n",
    "    return 1/(1 + np.exp(-z))\n",
    "\n",
    "print(sigmoid(z))"
   ]
